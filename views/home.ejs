<%- include("partials/header.ejs"); %>
<div class="container">
  <h1>Home</h1>
  <p><%= homeStarting %></p>
  <%post.forEach( function name(post) {%>
  <h1><%= post.title %></h1>
  <p>
    <%= post.content.substring(0, 100)+ "..."%>
    <a href="/posts/<%= post.title %>"">Read More</a>
  </p>
  <% }) %>
</div>
<%- include("partials/footer.ejs"); %>
